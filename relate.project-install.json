        },
        {
            "dumpFile": "data/twitter-trolls-40-40.dump",
            "targetNeo4jVersion": "4.0.* - 4.2.*",
            "plugins": ["apoc", "graph-data-science"]
        },
        {
            "dumpFile": "data/twitter-trolls-40-43.dump",
            "targetNeo4jVersion": "^4.3",
            "plugins": ["apoc", "graph-data-science"]
        }
    ]
}
